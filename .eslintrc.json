{
  "extends": ["next/core-web-vitals", "next/typescript"],
 "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "ignoreRestSiblings": true,
        "vars": "all", // Explicitly include all variables
        "caughtErrors": "none" // Ignore catch block errors
      }
    ]
  }
}
